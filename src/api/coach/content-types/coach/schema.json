{
  "kind": "collectionType",
  "collectionName": "coaches",
  "info": {
    "singularName": "coach",
    "pluralName": "coaches",
    "displayName": "Coach",
    "description": "Team coaches and trainers"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "role": {
      "type": "string",
      "default": "Trainer"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "teams": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::team.team",
      "mappedBy": "coaches"
    }
  }
}
